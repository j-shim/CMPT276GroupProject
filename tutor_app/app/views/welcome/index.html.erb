<%= render 'layouts/header' %>

<div>
<h2>User Login</h2>
<%= form_with scope: :user, url:login_path, local: true do |form| %>
  <p>
    <%= form.label :username %><br>
    <%= form.text_field :username %>
  </p>
 
  <p>
    <%= form.label :password %><br>
    <%= form.password_field :password %>
  </p>
 
  <p>
    <%= form.submit "Log in"%>
    
  </p>
 </div>
<% end %>
