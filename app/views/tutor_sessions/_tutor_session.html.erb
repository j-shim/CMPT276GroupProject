<li id="tutor_session-<%= tutor_session.id %>" class="tutor_session_list">
<span class="subject"><%= tutor_session.subject %></span>
<span class="date"><%= tutor_session.date.strftime("%a, %B %d, %Y") %></span>
<span class="time"><%= tutor_session.starttime.time.strftime("%I:%M %p") %> - <%= tutor_session.endtime.time.strftime("%I:%M %p") %></span>
<span class="time"><%= tutor_session.time_duration %> hours</span>
